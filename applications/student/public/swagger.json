{
	"openapi": "3.0.0",
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"IStudent": {
				"properties": {
					"school_name": {
						"type": "string"
					},
					"education": {
						"type": "string"
					},
					"grade": {
						"type": "number",
						"format": "double"
					},
					"student_card": {
						"type": "string"
					}
				},
				"required": [
					"school_name",
					"education",
					"grade",
					"student_card"
				],
				"type": "object",
				"additionalProperties": false
			},
			"IResponeUser": {
				"properties": {
					"first_name": {
						"type": "string"
					},
					"last_name": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"picture": {
						"type": "string",
						"nullable": true
					}
				},
				"required": [
					"first_name",
					"last_name",
					"picture"
				],
				"type": "object",
				"additionalProperties": false
			},
			"messageStudent": {
				"properties": {
					"message": {
						"type": "string"
					}
				},
				"required": [
					"message"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "student-service",
		"version": "1.0.0",
		"description": "Learnwithkru api project",
